{
  "name": "tree-sitter-toit",
  "version": "0.0.1",
  "description": "toit grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parsing",
    "incremental"
  ],
  "dependencies": {
    "nan": "^2.18.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.21.0-pre-release-1"
  },
  "scripts": {
    "test": "tree-sitter test",
    "generate": "tree-sitter generate",
    "test-parsing": "scripts/test-parsing",
    "parse": "tree-sitter parse"
  },
  "tree-sitter": [
    {
      "scope": "source.toit",
      "injection-regex": "toit",
      "file-types": [
        "toit"
      ]
    }
  ]
}
